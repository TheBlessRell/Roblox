local players = game:GetService("Players")
local player = players.LocalPlayer
local teams = game:GetService("Teams")
local replicatedStorage = game:GetService("ReplicatedStorage")
local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/hzhzhzsjjs/Zhzh/refs/heads/main/The%20ssi"))()

MakeWindow({
  Hub = {
    Title = "REUNION HUB",
    Animation = "BLUE LOCK"
  }
})

MinimizeButton({
  Image = "rbxassetid://84442594910404",
  Size = {50, 50},
  Color = Color3.fromRGB(255, 255, 255),
  Corner = true,
  Stroke = true,
  StrokeColor = Color3.fromHSV(0.5, 1, 1)
})

local Main = MakeTab({Name = "INFO"})

local Image = AddImageLabel(Main, {
  Name = "President Rell",
  Image = "rbxassetid://84442594910404"
})

local Paragraph1 = AddParagraph(Main, {"W RELL", "CREDIT"})

-- Styles tab
local Main = MakeTab({Name = "Styles"})

local styles = {
  "Isagi", "Chigiri", "Kurona", "Igaguri", "Hiori", "Gagamaru",
  "Otoya", "Bachira", "Nagi", "Reo", "Karasu", "Shidou",
  "Yukimiya", "NEL Bachira", "King", "Kunigami", "Aiku",
  "Rin", "Sae", "NEL Isagi", "Kaiser", "Don Lorenzo"
}

for _, styleName in ipairs(styles) do
  AddButton(Main, { 
    Name = styleName, 
    Description = "RELL DID IT", 
    Callback = function()
      local player = game.Players.LocalPlayer
      local playerStats = player:WaitForChild("PlayerStats", 5)

      if playerStats then
          local style = playerStats:FindFirstChild("Style")
          if style and style:IsA("StringValue") then
              style.Value = styleName
          else
              warn("Style not found or not a StringValue.")
          end
      else
          warn("PlayerStats not found.")
      end
    end
  })
end

AddButton(Main, { 
  Name = "Chigiri", 
  Description = "RELL DID IT", 
   Callback = function()
      local player = game.Players.LocalPlayer
local playerStats = player:WaitForChild("PlayerStats", 5) -- wait up to 5 seconds for PlayerStats

if playerStats then
    local style = playerStats:FindFirstChild("Style")
    if style and style:IsA("StringValue") then
        style.Value = "Chigiri"
    else
        warn("Style not found or not a StringValue.")
    end
else
    warn("PlayerStats not found.")
end
  end
}) 

